{
    "name": "funddy/fixture-bundle",
    "description": "FixtureBundle for Symfony2",
    "keywords": ["fixture bundle"],
    "homepage": "https://github.com/funddy/fixture-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Keyvan Akbary",
            "email": "keyvan@funddy.com",
            "homepage": "http://keyvanakbary.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "funddy/fixture": "1.0.*",
        "symfony/console": ">=2.0,<2.3-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.7.2"
    },
    "autoload": {
        "psr-0": { "Funddy\\Bundle\\FixtureBundle": "" }
    },
    "target-dir": "Funddy/Bundle/FixtureBundle",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
